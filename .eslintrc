{
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": 1,
    "comma-dangle": 0,
    "brace-style": [2, "stroustrup", { "allowSingleLine": true }],
    "new-cap": 0,
    "eol-last": 0,
    "no-undef": 2,
    "react/prop-types": 0,
    "no-console": 0,
    "quotes": 0
  },
  "environment": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb"
}