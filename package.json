{
  "name": "iswp2017",
  "version": "1.2.0",
  "description": "Web application for the Texas 2017 State Water Plan",
  "private": true,
  "main": "app/index.es",
  "scripts": {
    "start": "NODE_PATH=./app babel-node app/index.es",
    "webpack": "./node_modules/webpack/bin/webpack.js -p --config webpack.config.js",
    "dev-start": "concurrently \"yarn run dev-webpack\" \"yarn run dev-server\" ",
    "dev-server": "NODE_PATH=./app nodemon --exec babel-node -- app/index.es",
    "dev-webpack": "yarn run clean-dist && ./node_modules/webpack/bin/webpack.js -d --progress --colors --watch --config webpack.config.js",
    "test": "yarn run unit-test && yarn run integration-test",
    "unit-test": "NODE_PATH=./app tape test/unit/**.*",
    "integration-test": "NODE_PATH=./app lab -T test/integration/transform.js -S test/integration/",
    "dev-test": "nodemon --exec yarn run unit-test",
    "clean-dist": "rm -rf app/public/dist/*.js app/public/dist/*.map app/public/dist/*.css"
  },
  "engines": {
    "node": ">=4.8.4",
    "yarn": "1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tnris/iswp2017.git"
  },
  "author": "TNRIS",
  "contributors": [
    "James Seppi",
    "Andy Wilson"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tnris/iswp2017/issues"
  },
  "homepage": "https://github.com/tnris/iswp2017",
  "dependencies": {
    "alt": "^0.17.8",
    "assets-webpack-plugin": "^3.2.0",
    "axios": "^0.7.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "boom": "^2.10.1",
    "chartist": "0.9.4",
    "classnames": "^2.1.5",
    "condense-whitespace": "^1.0.0",
    "css-loader": "^0.28.7",
    "debounce": "^1.0.0",
    "dom-classlist": "^1.0.1",
    "extend": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "format-number": "^2.0.0",
    "good": "^6.4.0",
    "good-console": "^5.1.0",
    "hapi": "^9.3.1",
    "hapi-etags": "^0.1.0",
    "hapi-require-https": "^2.0.5",
    "hat": "0.0.3",
    "history": "1.13.1",
    "hoek": "^2.16.3",
    "inert": "^3.0.1",
    "intersperse": "^1.0.0",
    "is-firefox": "^1.0.3",
    "joi": "^6.7.0",
    "knex": "^0.10.0",
    "merge": "^1.2.0",
    "node-sass": "^4.5.3",
    "papaparse": "^4.1.2",
    "ramda": "^0.19.1",
    "react": "^0.14.5",
    "react-addons-linked-state-mixin": "^0.14.3",
    "react-addons-pure-render-mixin": "^0.14.0",
    "react-dom": "^0.14.1",
    "react-helmet": "^2.1.0",
    "react-pivot": "1.13.1",
    "react-router": "1.0.2",
    "react-select": "^0.9.1",
    "react-spinkit": "^1.1.4",
    "react-toggle-display": "^1.0.0",
    "reactable": "^0.12.0",
    "round-precision": "^1.0.0",
    "sass-loader": "^6.0.6",
    "scale-number-range": "^1.0.3",
    "sqlite3": "^3.0.10",
    "style-loader": "^0.18.2",
    "svg-react-loader": "^0.4.5",
    "swig": "^1.4.2",
    "titleize": "^1.0.0",
